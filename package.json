{
  "name": "n8n-nodes-pinelabs",
  "version": "0.1.0",
  "description": "n8n community node for Pine Labs Online Pay by Link",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "n8n-nodes",
    "pine-labs",
    "payment-link",
    "pay-by-link"
  ],
  "license": "MIT",
  "author": {
    "name": "Pine Labs",
    "email": "support@pinelabs.com"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts && eslint package.json",
    "lintfix": "eslint nodes credentials --ext .ts --fix && eslint package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PineLabsOnlineApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PineLabsOnline/PineLabsOnline.node.js"
    ]
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^2.6.0"
  }
}
